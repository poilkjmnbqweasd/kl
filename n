local gui = Instance.new("ScreenGui",game.Players.LocalPlayer.PlayerGui)local scra = Instance.new("TextBox",gui)scra.Size = UDim2.new(0,200,0,50)scra.PlaceholderText = "输入你要踢人的名称"local button = Instance.new("TextButton",gui)button.Text = "踢TA"button.Size = UDim2.new(0,200,0,50)button.Position = UDim2.new(0,0,0,80)local kick = scra.Textbutton.MouseButton1Click:Connect(function()	game.Players:WaitForChild(kick):Kick("An Admin Kicked for you")end)
